#. Translators: Summary/title for this add-on
msgid "Custom Labels"
msgstr "Benutzerdefinierte Beschriftungen"

#. Translators: Long description to be shown for this add-on
msgid "An NVDA addon that allows you to add custom labels for unlabeled controls and edit existing ones."
msgstr "Eine NVDA-Erweiterung, mit der Sie benutzerdefinierte Beschriftungen für unbeschriftete Steuerelemente hinzufügen und vorhandene bearbeiten können."

#. Translators: what's new content for the add-on version
msgid "Initial release of the Custom Labels add-on."
msgstr "Erstveröffentlichung der Erweiterung Benutzerdefinierte Beschriftungen."

#. Translators: Description for the set custom label script
msgid "Set or edit a custom label for the current control"
msgstr "Benutzerdefinierte Beschriftung für das aktuelle Steuerelement festlegen oder bearbeiten"

#. Translators: Error message when control cannot be labeled
msgid "Cannot label this type of control"
msgstr "Dieser Steuerelementtyp kann nicht beschriftet werden"

#. Translators: Error message when control cannot be identified
msgid "Cannot identify this control"
msgstr "Dieses Steuerelement kann nicht identifiziert werden"

#. Translators: Used when application name is unknown
msgid "Unknown"
msgstr "Unbekannt"

#. Translators: Confirmation when label is removed
msgid "Label removed"
msgstr "Beschriftung entfernt"

#. Translators: Message when there's no label to remove
msgid "No label to remove"
msgstr "Keine Beschriftung zum Entfernen vorhanden"

#. Translators: Confirmation when label is set
#, python-brace-format
msgid "Label set to: {label}"
msgstr "Beschriftung festgelegt auf: {label}"

#. Translators: Description for the remove custom label script
msgid "Remove the custom label from the current control"
msgstr "Die benutzerdefinierte Beschriftung vom aktuellen Steuerelement entfernen"

#. Translators: Description for the check label script
msgid "Check if current control has a custom label"
msgstr "Prüfen, ob das aktuelle Steuerelement eine benutzerdefinierte Beschriftung hat"

#. Translators: Message showing the custom label
#, python-brace-format
msgid "Custom label: {label}"
msgstr "Benutzerdefinierte Beschriftung: {label}"

#. Translators: Message when no custom label exists
#, python-brace-format
msgid "No custom label. Original: {name}"
msgstr "Keine benutzerdefinierte Beschriftung. Original: {name}"

#. Translators: Description for the manage labels script
msgid "Open custom labels settings"
msgstr "Einstellungen für benutzerdefinierte Beschriftungen öffnen"

#. Translators: Title of dialog when editing an existing label
msgid "Edit Custom Label"
msgstr "Benutzerdefinierte Beschriftung Bearbeiten"

#. Translators: Title of dialog when setting a new label
msgid "Set Custom Label"
msgstr "Benutzerdefinierte Beschriftung Festlegen"

#. Translators: Label for the control information list
msgid "Control &information:"
msgstr "Steuerelement-&Informationen:"

#. Translators: Column header for property name
msgid "Property"
msgstr "Eigenschaft"

#. Translators: Column header for property value
msgid "Value"
msgstr "Wert"

#. Translators: Displayed when control has no name
msgid "(unlabeled)"
msgstr "(unbeschriftet)"

#. Translators: Property name for original control name
msgid "Original name"
msgstr "Originalname"

#. Translators: Property name for control role
msgid "Role"
msgstr "Rolle"

#. Translators: Property name for application
msgid "Application"
msgstr "Anwendung"

#. Translators: Property name for control identifier
msgid "Identifier"
msgstr "Bezeichner"

#. Translators: Property name for current custom label
msgid "Current label"
msgstr "Aktuelle Beschriftung"

#. Translators: Label for the custom label input field
msgid "&Custom label:"
msgstr "&Benutzerdefinierte Beschriftung:"

#. Translators: OK button
msgid "&OK"
msgstr "&OK"

#. Translators: Cancel button
msgid "&Cancel"
msgstr "&Abbrechen"

#. Translators: Remove button to delete the label
msgid "&Remove"
msgstr "&Entfernen"

#. Translators: Label for the tree of custom labels
msgid "&Available custom labels:"
msgstr "&Verfügbare benutzerdefinierte Beschriftungen:"

#. Translators: Edit button
msgid "&Edit"
msgstr "&Bearbeiten"

#. Translators: Remove app button
msgid "Remove A&pp"
msgstr "A&nwendung Entfernen"

#. Translators: Remove all button
msgid "Remove &All"
msgstr "&Alle Entfernen"

#. Translators: Root item in the labels tree
msgid "All Labels"
msgstr "Alle Beschriftungen"

#. Translators: App node showing app name and label count
#, python-brace-format
msgid "{app} ({count} labels)"
msgstr "{app} ({count} Beschriftungen)"

#. Translators: Label display format with identifier
#, python-brace-format
msgid "{label} - {identifier}"
msgstr "{label} - {identifier}"

#. Translators: Shown when original name is not available
msgid "(not available from saved data)"
msgstr "(in gespeicherten Daten nicht verfügbar)"

#. Translators: Confirmation prompt to remove a label
#, python-brace-format
msgid "Remove label '{label}'?"
msgstr "Beschriftung '{label}' entfernen?"

#. Translators: Title for removal confirmation dialog
msgid "Confirm Removal"
msgstr "Entfernen Bestätigen"

#. Translators: Confirmation prompt to remove all labels for an app
#, python-brace-format
msgid "Remove all {count} labels for '{app}'?"
msgstr "Alle {count} Beschriftungen für '{app}' entfernen?"

#. Translators: Confirmation prompt to remove all custom labels
#, python-brace-format
msgid "Remove all {count} custom labels?"
msgstr "Alle {count} benutzerdefinierten Beschriftungen entfernen?"
